                "tags": [
                    "Users"
                ],
                "summary": "Edit profile user",
                "description": "edit profile user",
                "operationId": "50c366241588acdb6b422cffa7997575",
                "requestBody": {
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "format": "username",
                                        "example": "alalisod"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Aliii"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful Edited info!!"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        }
